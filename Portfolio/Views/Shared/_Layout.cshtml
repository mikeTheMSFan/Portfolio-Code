<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"/>
    @await RenderSectionAsync("Metadata", false)

    <!--Title Page-->
    <title>@ViewData["Title"]</title>

    <!-- Fontfaces CSS-->
    <link href="~/css/layout/font-face.min.css" rel="stylesheet" media="all"/>
    <link href="~/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" media="all"/>
    <link href="~/vendor/linearicons-free/css/linearicons-free.css" rel="stylesheet" media="all"/>
    <link href="~/vendor/elegant-icons/css/elegant-icons.css" rel="stylesheet" media="all"/>

    <!-- Bootstrap CSS-->
    <link href="~/vendor/bootstrap/bootstrap.min.css" rel="stylesheet" media="all"/>

    <!-- Vendor CSS-->
    <link href="~/vendor/animate.css/animate.min.css" rel="stylesheet" media="all"/>
    <link href="~/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all"/>
    <link href="~/vendor/slick/slick.css" rel="stylesheet" media="all"/>
    <link href="~/vendor/animsition/animsition.min.css" rel="stylesheet" media="all"/>
    <link href="~/vendor/lightbox/css/lightbox.min.css" rel="stylesheet" media="all"/>
    <link href="~/vendor/revolution/css/layers.css" rel="stylesheet"/>
    <link href="~/vendor/revolution/css/navigation.css" rel="stylesheet"/>
    <link href="~/vendor/revolution/css/settings.css" rel="stylesheet"/>

    <!--Custom CSS-->
    @await RenderSectionAsync("CustomCss", false);

    <!-- Main CSS-->
    <link href="~/css/layout/layout.min.css" rel="stylesheet" media="all"/>

    <!-- Jquery JS-->
    <script src="~/vendor/jquery/jquery.min.js"></script>

    <!--Favicons-->
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
</head>
<body class="animsition page-wrapper">
<header class="header">
    <!-- header desktop-->
    <div class="header-primary header-fixed d-lg-block d-none">
        <div class="container-fluid">
            <div class="section-inner header-bar">
                <div class="header-bar-logo">
                    <a class="logo-link" asp-controller="Home" asp-action="Index">
                        <img src="~/imgs/michael-robinson.jpg" alt="Michael Robinson"/>
                        <span>MICHAEL ROBINSON</span>
                    </a>
                </div>
                <div class="header-bar-menu">
                    <nav class="navbar-primary">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link" asp-action="Index" asp-controller="Home">HOME</a>
                            </li>
                            <li class="nav-item">
                                    <a id="aboutMeLink" class="nav-link chosen-single" asp-controller="home" asp-action="Index" asp-fragment="about-me">ABOUT ME</a>
                            </li>
                            <li class="nav-item">
                                    <a id="portfolioLink" class="nav-link chosen-single" asp-controller="home" asp-action="Index" asp-fragment="portfolio">PORTFOLIO</a>
                            </li>
                            <li class="nav-item">
                                    <a id="blogsLink" class="nav-link" asp-action="Index" asp-controller="Blogs">BLOGS</a>
                            </li>
                            <partial name="_LoginPartial"/>
                        </ul>
                    </nav>
                </div>
                <div class="header-bar-featured d-none d-xl-block">
                        <a id="contact-button" class="au-btn au-btn-radius au-btn-primary" asp-controller="Home" asp-action="Index" asp-fragment="contact-me">CONTACT ME</a>
                </div>
            </div>
        </div>
    </div>
    <!-- end header desktop-->

    <!-- header mobile-->
    <div class="header-mobile header-fixed d-lg-none">
        <div class="container-fluid">
            <div class="header-bar">
                <div class="header-bar-logo">
                    <a class="logo-link" asp-controller="Home", asp-action="Index">
                        <img src="~/imgs/michael-robinson.jpg" alt="Michael Robinson"/>
                        <span>MICHAEL ROBINSON</span>
                    </a>
                </div>
                <div class="header-bar-menu">
                    <button class="handler-slidebar hamburger" type="button">
                        <span class="hamburger-box">
                            <span class="hamburger-inner"></span>
                        </span>
                    </button>
                </div>
            </div>
        </div>
        <div class="header-slidebar header-slidebar-primary closed">
            <div class="header-slidebar-inner">
                <div class="box-author">
                    <a href="index.html">
                        <div class="author-image">
                            <img src="~/imgs/michael-robinson.jpg" alt="Michael Robinson"/>
                        </div>
                        <h3 class="author-name">MICHAEL ROBINSON</h3>
                    </a>
                </div>
                <nav class="navbar-vertical navbar-vertical-white">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" asp-action="Index" asp-controller="Home">HOME</a>
                        </li>
                        <li class="nav-item">
                            <a id="aboutMeLink" class="nav-link chosen-single" asp-controller="home" asp-action="Index" asp-fragment="about-me">ABOUT ME</a>
                        </li>
                        <li class="nav-item">
                            <a id="portfolioLink" class="nav-link chosen-single" asp-controller="home" asp-action="Index" asp-fragment="portfolio">PORTFOLIO</a>
                        </li>
                        <li class="nav-item">
                            <a id="blogsLink" class="nav-link" asp-action="Index" asp-controller="Blogs">BLOGS</a>
                        </li>
                        <partial name="_LoginPartial"/>
                    </ul>
                </nav>
                <div class="slidebar-footer">
                    <ul class="socials h-list">
                        <li class="social-item">
                            <a class="fa fa-envelope" href="mailto:michael.robinson@mikemrobinsondev.com" data-toggle="tooltip" title="Email"></a>
                        </li>
                        <li class="social-item">
                            <a class="fa fa-twitter" href="https://twitter.com/MichaelMRobins4" data-toggle="tooltip" title="Twitter"></a>
                        </li>
                        <li class="social-item">
                            <a class="fa fa-linkedin" href="https://www.linkedin.com/in/michael-maurice-robinson/" data-toggle="tooltip" title="Linkedin"></a>
                        </li>
                        <li class="social-item">
                            <a class="fa fa-github" href="https://github.com/mikeTheMSFan" data-toggle="tooltip" title="GitHub"></a>
                        </li>
                    </ul>
                    <p class="fo-copy">@@ 2023 Michael Robinson</p>
                </div>
            </div>
        </div>
    </div>
    <!-- end header mobile-->
</header>
@if (TempData.ContainsKey("StatusMessage"))
{
    @await Html.PartialAsync("_StatusMessage",
               TempData["StatusMessage"] as string)
}

<main id="mainContent mb-0">
    @RenderBody()
</main>

<!-- footer-->
<footer class="footer footer-primary static-bottom bg-dark-2 p-t-30 p-b-30">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-md-6 fo-left">
                <p class="fo-copy">@@ 2023 Michael Robinson</p>
            </div>
            <div class="col-md-6 fo-right">
                <ul class="socials h-list">
                    <li class="social-item">
                        <a class="fa fa-envelope" href="mailto:michael.robinson@mikemrobinsondev.com" data-toggle="tooltip" title="Email"></a>
                    </li>
                    <li class="social-item">
                        <a class="fa fa-twitter" href="https://twitter.com/MichaelMRobins4" data-toggle="tooltip" title="Twitter"></a>
                    </li>
                    <li class="social-item">
                        <a class="fa fa-linkedin" href="https://www.linkedin.com/in/michael-maurice-robinson/" data-toggle="tooltip" title="Linkedin"></a>
                    </li>
                    <li class="social-item">
                        <a class="fa fa-github" href="https://github.com/mikeTheMSFan" data-toggle="tooltip" title="GitHub"></a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</footer>
<!-- end footer-->

<div id="up-to-top">
    <i class="fa fa-angle-up"></i>
</div>

<!-- Bootstrap JS-->
<script src="~/vendor/popper.js/popper.min.js"></script>
<script src="~/vendor/bootstrap/bootstrap.min.js"></script>
<!-- Vendor JS-->
<script src="~/vendor/slick/slick.min.js"></script>
<script src="~/vendor/animsition/animsition.min.js"></script>
<script src="~/vendor/waypoints/jquery.waypoints.min.js"></script>
<script src="~/vendor/jquery.counterup/jquery.counterup.min.js"></script>
<script src="~/vendor/lightbox/js/lightbox.min.js"></script>
<script src="~/vendor/isotope/isotope.pkgd.min.js"></script>
<script src="~/vendor/wowjs/wow.min.js"></script>
<script src="~/vendor/revolution/js/jquery.themepunch.tools.min.js"></script>
<script src="~/vendor/revolution/js/jquery.themepunch.revolution.min.js"></script>
<script src="~/js/layout/config-revolution-slider.min.js"></script>

<!--Custom JS-->
@await RenderSectionAsync("Scripts", false)

<!-- Main JS-->
<script src="~/js/layout/global.js"></script>

</body>
</html>